<ui-display title='Splitter' button> 
	<ui-section>
		{{#each data.reagents: reagent}}
			<span>{{reagent}} : {{data.reagents[reagent]}}</span>
			<ui-button action='remove' params='reagent'>Remove</ui-button><br>
		{{/each}}
		<ui-button action='add'>Add</ui-button><br>
	</ui-section>
</ui-display>